{
  "version": 2,
  "builds": [
    { "src": "ui/**", "use": "@vercel/static" }
  ],
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/ui/index.html" },
    { "source": "/ui/:path*", "destination": "/ui/:path*" },
    { "source": "/:path*", "destination": "/ui/:path*" }
  ]
}
